{
    "version": "0.2.0",
    "configurations": 
    [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch",
            "skipFiles": 
            [
                "<node_internals>/**"
            ],
			"windows":
			{
				"program": "${workspaceFolder}\\src\\index.js",
				"cwd": "${workspaceFolder}\\src",
			},
			"linux":
			{
				"program": "${workspaceFolder}/src/index.js",
				"cwd": "${workspaceFolder}/src",
			},
			"osx":
			{
				"program": "${workspaceFolder}/src/index.js",
				"cwd": "${workspaceFolder}/src",
			},
			"console": "integratedTerminal",
			"outFiles": 
			[
				"${workspaceFolder}/**/*.js",
				"!**/node_modules/**",
			]
        },
    ],
}